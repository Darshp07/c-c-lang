//  Write a C Program to Find the Grade of a Student Using Switch Case
#include <stdio.h>

int main() {
    int score;
    char grade;
    printf("enter your score:");
scanf("%d",&score);
if (score>=90){
    grade ='A';
}else if (score>=80 ){
    grade = 'B';
}else if (score>=70 ){
    grade = 'C';
}else if (score>=60 ){
    grade = 'D';
}else if (score>=50){
    grade = 'E';
}else if (score>=40){
    grade = 'F';
}else {
    grade = 'G';
}
switch (grade){
    case 'A':
    printf("grade : a");
    break;
       case 'B':
    printf("grade : b");
    break;
       case 'C':
    printf("grade : c");
    break;
       case 'D':
    printf("grade : d");
    break;
       case 'E':
    printf("grade : e");
    break;
       case 'F':
    printf("grade : f");
    break;
       case 'G':
    printf("grade : g");
}
    return 0;
}